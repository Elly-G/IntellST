  <keepDependencies>false</keepDependencies>
  <properties/>
  <definition class="org.jenkinsci.plugins.workflow.cps.CpsFlowDefinition" plugin="workflow-cps@2.81">
    <script>pipeline {
  agent any
 
  tools {nodejs &quot;node&quot;}
 
  stages {
    
    stage(&apos;run tests&apos;) {
      steps {
        git branch: &apos;front-jenkins&apos;, url: &quot;https://github.com/Elly-G/IntellST.git&quot;
        dir(&quot;intellst-front/output/coverage/jest/&quot;) {
        sh &apos;pwd&apos;
        sh &apos;ls&apos;
        sh &apos;npm install&apos;
        sh &apos;npm run test&apos;
        sh &apos;cp clover.xml ./clover.xml&apos;
        }
      }
    }
  }
}</script>
    <sandbox>true</sandbox>
  </definition>
  <triggers/>
  <disabled>false</disabled>
</flow-definition>